
	config := meta.(*Config)

	// current import_formats can't import fields with forward slashes in their value
	if err := parseImportId([]string{"(?P<name>.+)"}, d, config); err != nil {
		return nil, err
	}

	stringParts := strings.Split(d.Get("name").(string), "/")
	if len(stringParts) != 6 {
		return nil, fmt.Errorf(
				"Saw %s when the name is expected to have shape %s",
				d.Get("name"),
				"projects/{{project_id}}/locations/{{location_id}}/instances/default",
			)
	}

	if err := d.Set("project_id", stringParts[1]); err != nil {
		return nil, fmt.Errorf("Error setting project_id: %s", err)
	}
	if err := d.Set("location_id", stringParts[3]); err != nil {
		return nil, fmt.Errorf("Error setting location_id: %s", err)
	}
	return []*schema.ResourceData{d}, nil
